<div id="content_detail">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.work_has_work')) -%></h1>
<div id="content_list">

<% form_for(@work_has_work) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label t('activerecord.attributes.work_has_work.from_work') -%><br />
    <%= f.text_field :from_work_id -%>
  </p>
  <p>
    <%= f.label t('activerecord.attributes.work_has_work.to_work') -%><br />
    <%= f.text_field :to_work_id -%>
  </p>
  <p>
    <%= f.label :type -%><br />
    <%= f.text_field :type -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<% end -%>
</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @work_has_work -%></li>
    <li><%= link_to t('page.back'), work_has_works_path -%></li>
  </ul>
</div>
