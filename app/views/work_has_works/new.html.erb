<h1><%= t('page.new', :model => t('activerecord.models.work_has_work')) -%></h1>

<% form_for(@work_has_work) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :from_work_id -%><br />
    <%= f.text_field :from_work_id -%>
  </p>
  <p>
    <%= f.label :to_work_id -%><br />
    <%= f.text_field :to_work_id -%>
  </p>
  <p>
    <%= f.label :type -%><br />
    <%= f.text_field :type -%>
  </p>
  <p>
    <%= f.label :position -%><br />
    <%= f.text_field :position -%>
  </p>
  <p>
    <%= f.submit t('page.create') -%>
  </p>
<% end -%>

<%= link_to t('page.back'), work_has_works_path -%>
