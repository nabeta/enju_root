<div id="content_detail">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.advertise')) -%></h1>
<div id="content_list">

<%- form_for(@advertise) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :patron_id -%><br />
    <%= f.text_field :patron_id -%>
  </p>
  <p>
    <%= f.label :advertisement_id -%><br />
    <%= f.text_field :advertisement_id -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>

</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @advertise -%></li>
    <li><%= link_to t('page.back'), advertises_path -%></li>
  </ul>
</div>
