<div id="content_detail" class="ui-corner-all">
<h1 class="title">Listing expression_relationship_types</h1>
<div id="content_list">

<table class="index">
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
  </tr>

<% @expression_relationship_types.each do |expression_relationship_type| %>
  <tr class="line<%= cycle("0", "1") -%>">
    <td><%= link_to expression_relationship_type.name, expression_relationship_type %></td>
    <td><%= link_to t('enju.edit'), edit_expression_relationship_type_path(expression_relationship_type) %></td>
    <td><%= link_to t('enju.destroy'), expression_relationship_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to 'New Content type', new_expression_relationship_type_path %></li>
  </ul>
</div>
