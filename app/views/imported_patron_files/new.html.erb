<div id="submenu">
  <ul>
    <li><%= link_to t('page.back'), imported_patron_files_path -%></li>
  </ul>
</div>

<div id="content_detail">
  <h1><%= ('New imported_patron_file') -%></h1>

<%- form_for(@imported_patron_file, :html => { :multipart => true }) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <b><%= ('File') -%></b><br />
    <%= f.file_field :uploaded_data -%>
  </p>

  <p>
    <%= f.submit t('page.create') -%>
  </p>
<%- end -%>

</div>
