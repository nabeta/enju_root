<div id="content_detail">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.shelf_has_manifestation')) -%></h1>
<div id="content_list">

<% form_for(@shelf_has_manifestation) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label t('activerecord.attributes.shelf_has_manifestation.shelf') -%><br />
    <%= f.text_field :shelf_id %>
  </p>
  <p>
    <%= f.label t('activerecord.attributes.shelf_has_manifestation.manifestation') -%><br />
    <%= f.text_field :manifestation_id %>
  </p>
  <p>
    <%= f.submit t('page.update') %>
  </p>
<% end %>
</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @shelf_has_manifestation %></li>
    <li><%= link_to t('page.back'), shelf_has_manifestations_path %></li>
  </ul>
</div>
