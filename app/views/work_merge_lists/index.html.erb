<div id="submenu">
  <ul>
    <li><%= link_to ('New work_merge_list'), new_work_merge_list_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1><%= ('Listing work_merge_lists') -%></h1>

<table id="index">
  <tr>
    <th><%= t('page.title') -%></th>
    <th></th>
  </tr>

<%- for work_merge_list in @work_merge_lists -%>
  <tr class="line<%= cycle("0", "1") -%>">
    <td><%= link_to h(work_merge_list.title), work_merge_list -%></td>
    <td><%= link_to t('page.show'), work_merge_list -%></td>
    <td><%= link_to t('page.edit'), edit_work_merge_list_path(work_merge_list) -%></td>
    <td><%= link_to t('page.destroy'), work_merge_list, :confirm => t('page.are_you_sure'), :method => :delete -%></td>
  </tr>
<%- end -%>
</table>

</div>
