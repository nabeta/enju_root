<div id="content_detail">
<h1 class="title">Listing expression_has_expressions</h1>
<div id="content_list">

<table id="index">
  <tr>
    <th>From expression</th>
    <th>To expression</th>
    <th>Expression Relationship type</th>
    <th>Position</th>
  </tr>

<% for expression_has_expression in @expression_has_expressions -%>
  <tr>
    <td><%= link_to h(expression_has_expression.from_expression.original_title), expression_has_expression.from_expression -%></td>
    <td><%= link_to h(expression_has_expression.to_expression.original_title), expression_has_expression.to_expression -%></td>
    <td><%=h expression_has_expression.type -%></td>
    <td><%=h expression_has_expression.position -%></td>
    <td><%= link_to t('page.show'), expression_has_expression -%></td>
    <td><%= link_to t('page.edit'), edit_expression_has_expression_path(expression_has_expression) -%></td>
    <td><%= link_to t('page.destroy'), expression_has_expression, :confirm => t('page.are_you_sure'), :method => :delete -%></td>
  </tr>
<% end -%>
</table>

<%= will_paginate(@expression_has_expressions) -%>
<br />

</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.new', :model => t('activerecord.models.expression_has_expression')), new_expression_has_expression_path -%></li>
  </ul>
</div>
