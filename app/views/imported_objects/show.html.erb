<div class="inner">
<h1 class="title"><%= t('page.showing', :model => t('activerecord.models.imported_object')) -%></h1>
<div class="inner">
<p>
  <b><%= t('activerecord.attributes.imported_object.imported_file') -%></b>
  <%= link_to h("#{@imported_object.imported_file_type} / #{@imported_object.imported_file_id}"), @imported_object.imported_file -%>
</p>

<p>
  <b><%= t('activerecord.attributes.imported_object.importable') -%></b>
  <%= link_to h("#{@imported_object.importable_type} / #{@imported_object.importable_id}"), @imported_object.importable -%>
</p>

</div>
</div>

<div id="sidebar">
  <ul>
    <li><%= link_to t('page.edit'), edit_imported_object_path(@imported_object) -%></li>
    <li><%= link_to t('page.back'), imported_objects_path -%></li>
  </ul>
</div>
