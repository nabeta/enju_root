<div id="submenu">
  <ul>
    <li><%= link_to t('page.new', :model => t('activerecord.models.patron_merge_list')), new_patron_merge_list_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1><%= t('page.listing', :model => t('activerecord.models.patron_merge_lists')) -%></h1>

<table id="index">
  <tr>
    <td><%= t('activerecord.attributes.patron_merge_list.title') -%></td>
    <td><%= t('page.created_at') -%></td>
  </tr>

<%- for patron_merge_list in @patron_merge_lists -%>
  <tr class="line<%= cycle("0", "1") -%>">
    <td><%= link_to h(patron_merge_list.title), patron_merge_list -%></td>
    <td><%=h patron_merge_list.created_at -%></td>
    <td><%= link_to t('page.show'), patron_merge_list -%></td>
    <td><%= link_to t('page.edit'), edit_patron_merge_list_path(patron_merge_list) -%></td>
    <td><%= link_to t('page.destroy'), patron_merge_list, :confirm => t('page.are_you_sure'), :method => :delete -%></td>
  </tr>
<%- end -%>
</table>

</div>
