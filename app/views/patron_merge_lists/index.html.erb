<div id="submenu">
  <ul>
    <li><%= link_to ('New patron_merge_list'), new_patron_merge_list_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1><%= ('Listing patron_merge_lists') -%></h1>

<table style="width: 100%">
  <tr>
    <td><%= ('Title') -%></td>
    <td><%= ('Created at') -%></td>
  </tr>

<%- for patron_merge_list in @patron_merge_lists -%>
  <tr class="line<%= cycle("0", "1") -%>">
    <td><%= link_to h(patron_merge_list.title), patron_merge_list -%></td>
    <td><%=h patron_merge_list.created_at -%></td>
    <td><%= link_to t('show'), patron_merge_list -%></td>
    <td><%= link_to t('edit'), edit_patron_merge_list_path(patron_merge_list) -%></td>
    <td><%= link_to t('destroy'), patron_merge_list, :confirm => ('Are you sure?'), :method => :delete -%></td>
  </tr>
<%- end -%>
</table>

</div>
