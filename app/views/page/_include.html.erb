<%= javascript_include_tag 'jquery', 'jquery-ui' -%>
<script type="text/javascript">
    jQuery.noConflict();
</script>
<%= javascript_include_tag :defaults -%>
<%= javascript_include_tag 'yui/build/yahoo/yahoo-min',
    'yui/build/yahoo-dom-event/yahoo-dom-event',
    'yui/build/element/element-min',
    'yui/build/connection/connection-min',
    'yui/build/container/container_core-min',
    'yui/build/tabview/tabview-min',
    'yui/build/treeview/treeview-min',
    'yui/build/history/history-min',
    'yui/build/menu/menu-min',
    :cache => true
-%>
<%= javascript_include_tag 'scriptaculous', 'builder', 'thickbox', 'in_place_editor_with_empty_text', :cache => 'include' -%>
<%= stylesheet_link_tag 'yui/build/reset-fonts-grids/reset-fonts-grids',
    'yui/build/fonts/fonts-min',
    'yui/build/base/base-min',
    :cache => true
-%>
<%= stylesheet_link_tag 'yui/build/assets/skins/sam/tabview',
    'yui/build/assets/skins/sam/treeview',
    'yui/build/assets/skins/sam/menu'
-%>
<%= stylesheet_link_tag 'thickbox', 'scaffold', 'nonopac', 'acts_as_taggable_stylesheet', 'thickbox_login', :cache => 'include' -%>
<%= calendar_date_select_includes -%>
<script type="text/javascript">
  YAHOO.util.Event.onContentReady("services", function () {
    var oMenuBar = new YAHOO.widget.MenuBar("services", { 
                                                    autosubmenudisplay: true, 
                                                    hidedelay: 750, 
                                                    lazyload: true });
    oMenuBar.render();
  });
</script>
