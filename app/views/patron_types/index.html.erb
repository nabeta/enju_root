<div id="submenu">
  <ul>
    <li><%= link_to 'New patron_type', new_patron_type_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1><%= ('Listing patron_types') -%></h1>

<table style="width: 100%">
  <tr>
    <th><%= ('Name') -%></th>
    <th><%= ('Display name') -%></th>
    <th><%= ('Note') -%></th>
  </tr>

<%- for patron_type in @patron_types -%>
  <tr class="line<%= cycle("0", "1") -%>">
    <td><%= link_to h(patron_type.name), patron_type -%></td>
    <td><%= link_to h(patron_type.display_name), patron_type -%></td>
    <td><%=h patron_type.note -%></td>
    <td><%= link_to t('edit'), edit_patron_type_path(patron_type) -%></td>
    <td><%= link_to t('destroy'), patron_type, :confirm => ('Are you sure?'), :method => :delete -%></td>
  </tr>
<%- end -%>
</table>

</div>
