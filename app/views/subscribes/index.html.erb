<div id="submenu">
  <ul>
    <li><%= link_to 'New subscribe', new_subscribe_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1>Listing subscribes</h1>

<table id="index">
  <tr>
    <th><%= ('Subscription') -%></th>
    <th><%= t('page.expression') -%></th>
  </tr>

<%- for subscribe in @subscribes -%>
  <tr class="line<%= cycle("0", "1") -%>">
    <td><%= link_to h(subscribe.subscription.title), subscribe.subscription -%></td>
    <td><%= link_to h(subscribe.expression.original_title), subscribe.expression -%></td>
    <td><%= link_to t('page.show'), subscribe -%></td>
    <td><%= link_to t('page.edit'), edit_subscribe_path(subscribe) -%></td>
    <td><%= link_to t('page.destroy'), subscribe, :confirm => t('page.are_you_sure'), :method => :delete -%></td>
  </tr>
<%- end -%>
</table>

</div>
