<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @manifestation_checkout_stat -%></li>
    <li><<%= link_to t('page.back'), manifestation_checkout_stats_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1>Editing manifestation_checkout_stat</h1>

<% form_for(@manifestation_checkout_stat) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :from_date -%><br />
    <%= f.datetime_select :from_date -%>
  </p>
  <p>
    <%= f.label :to_date -%><br />
    <%= f.datetime_select :to_date -%>
  </p>
  <p>
    <%= f.label :note -%><br />
    <%= f.text_area :note, :class => 'resource_textarea' -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<% end -%>

</div>
