<h1>Listing expression_relationships</h1>

<table>
  <tr>
    <th>Parent</th>
    <th>Child</th>
    <th>Expression relationship type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @expression_relationships.each do |expression_relationship| %>
  <tr>
    <td><%= expression_relationship.parent_id %></td>
    <td><%= expression_relationship.child_id %></td>
    <td><%= expression_relationship.expression_relationship_type_id %></td>
    <td><%= link_to 'Show', expression_relationship %></td>
    <td><%= link_to 'Edit', edit_expression_relationship_path(expression_relationship) %></td>
    <td><%= link_to 'Destroy', expression_relationship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

