<div id="content_detail">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.patron_has_patron')) -%></h1>
<div id="content_list">

<% form_for(@patron_has_patron) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :from_patron_id -%><br />
    <%= f.text_field :from_patron_id -%>
  </p>
  <p>
    <%= f.label :to_patron_id -%><br />
    <%= f.text_field :to_patron_id -%>
  </p>
  <p>
    <%= f.label :type -%><br />
    <%= f.text_field :type -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<% end -%>
</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @patron_has_patron -%></li>
    <li><%= link_to t('page.back'), patron_has_patrons_path -%></li>
  </ul>
</div>
