<div id="content_detail">
<h1 class="title"><%= t('page.new', :model => t('activerecord.models.patron_has_patron')) -%></h1>
<div id="content_list">

<% form_for(@patron_has_patron) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :from_patron_id -%><br />
    <%- if @patron_has_patron.from_patron -%>
      <%= link_to h(@patron_has_patron.from_patron.full_name), @patron_has_patron.from_patron -%>
      <%= f.hidden_field :from_patron_id -%>
    <%- else -%>
      <%= f.text_field :from_patron_id -%>
    <%- end -%>
  </p>
  <p>
    <%= f.label :to_patron_id -%><br />
    <%- if @patron_has_patron.to_patron -%>
      <%= link_to h(@patron_has_patron.to_patron.full_name), @patron_has_patron.to_patron -%>
      <%= f.hidden_field :to_patron_id -%>
    <%- else -%>
      <%= f.text_field :to_patron_id -%>
    <%- end -%>
  </p>
  <p>
    <%= f.label :type -%><br />
    <%= f.text_field :type -%>
  </p>
  <p>
    <%= f.submit t('page.create') -%>
  </p>
<% end -%>

</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.back'), patron_has_patrons_path -%></li>
  </ul>
</div>
