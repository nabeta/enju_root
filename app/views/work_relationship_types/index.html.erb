<h1>Listing work_relationship_types</h1>

<table id="index">
  <tr>
    <th>Name</th>
    <th>Display name</th>
    <th>Note</th>
    <th>Position</th>
  </tr>

<% for work_relationship_type in @work_relationship_types %>
  <tr>
    <td><%=h work_relationship_type.name %></td>
    <td><%=h work_relationship_type.display_name %></td>
    <td><%=h work_relationship_type.note %></td>
    <td><%=h work_relationship_type.position %></td>
    <td><%= link_to t('page.show'), work_relationship_type %></td>
    <td><%= link_to t('page.edit'), edit_work_relationship_type_path(work_relationship_type) %></td>
    <td><%= link_to t('page.destroy'), work_relationship_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New work_relationship_type', new_work_relationship_type_path %>
